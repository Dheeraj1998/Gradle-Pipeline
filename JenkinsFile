pipeline {
	agent any 
		stages {
			stage ("Clean"){
				steps {
					echo 'Cleaning step.'
					sh './gradlew clean --no-daemon'
				}
			}

			stage ("Build"){
				steps {
					echo 'Building step.'
					sh './gradlew build --no-daemon'
				}
			}

			stage ("Test"){
				steps {
					echo 'Testing step.'
				}
			}

			stage ("Deploy"){
				steps {
					echo 'Deploying step.'
				}
			}
		}
	
}